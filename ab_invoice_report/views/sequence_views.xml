<odoo>
    <data noupdate="1">
        <record id="sequence_sale_order" model="ir.sequence">
            <field name="name">Sale Order Sequence</field>
            <field name="code">sale.order.sequence</field>
            <!-- <field name="prefix">LUI/</field> -->
            <field name="suffix">/LUI-SO/%(year)s</field>
            <field name="padding">4</field>
            <field name="number_next_actual">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="sequence_account_move" model="ir.sequence">
            <field name="name">Account Move Sequence</field>
            <field name="code">account.move.sequence</field>
            <field name="suffix">/LUI-INV/%(year)s</field>
            <field name="padding">4</field>
            <field name="number_next_actual">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="sequence_bank" model="ir.sequence">
            <field name="name">Bank Sequence</field>
            <field name="code">bank.sequence</field>
            <!-- <field name="suffix">/LUI-INV/%(year)s</field> -->
            <field name="padding">4</field>
            <field name="number_next_actual">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="sequence_cash" model="ir.sequence">
            <field name="name">Cash Sequence</field>
            <field name="code">cash.sequence</field>
            <!-- <field name="suffix">/LUI-INV/%(year)s</field> -->
            <field name="padding">4</field>
            <field name="number_next_actual">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>